{
  "extends": "@react-native-paper-abstracted/typescript-config/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/lib/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    "next.config.js",
    ".next/types/**/*.ts",
    "mdx-components.tsx",
    "../.."
  ],
  "exclude": [
    "node_modules",
    "__examples__",
    "__components__",
    "../../packages/core",
    "../../packages/example",
  ]
}